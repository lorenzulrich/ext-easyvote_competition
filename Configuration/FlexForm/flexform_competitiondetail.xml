<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Konfiguration</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.competition>
						<TCEforms>
							<label>Wettbewerb</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:examples/locallang_db.xml:examples.pi_flexform.choosePage</numIndex>
									</numIndex>
								</items>
								<foreign_table>tx_easyvotecompetition_domain_model_competition</foreign_table>
								<foreign_table_where>AND tx_easyvotecompetition_domain_model_competition.deleted = 0 AND tx_easyvotecompetition_domain_model_competition.hidden = 0 ORDER BY title_short</foreign_table_where>
								<minitems>1</minitems>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</settings.competition>
					<settings.hideCompetitionIntroduction>
						<TCEforms>
							<label>Wettbewerbs-Einleitung verbergen</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.hideCompetitionIntroduction>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>